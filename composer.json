{
    "name": "dragon-code/laravel-cache",
    "description": "An improved interface for working with cache",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "cache",
        "helper",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://dragon-code.pro"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/laravel-cache/issues",
        "source": "https://github.com/TheDragonCode/laravel-cache"
    },
    "funding": [
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.2",
        "dragon-code/support": "^6.11.3",
        "illuminate/contracts": "^11.23 || ^12.0",
        "illuminate/support": "^11.23 || ^12.0"
    },
    "require-dev": {
        "nesbot/carbon": "^2.62 || ^3.0",
        "orchestra/testbench": "^9.0 || ^10.0",
        "phpunit/phpunit": "^11.0 || ^12.0"
    },
    "conflict": {
        "andrey-helldar/cache": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true,
            "symfony/thanks": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\Cache\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}